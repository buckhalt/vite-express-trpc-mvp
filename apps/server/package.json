{
   "name": "@react-vite-trpc/server",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "start": "cross-env NODE_ENV=production node dist/src/server.js",
      "pm2:start": "pnpm pm2:delete && pnpm build && cross-env NODE_ENV=test pm2 start dist/src/server.js --name @react-vite-trpc/server",
      "pm2:delete": "pm2 delete all &",
      "pm2:logs": "pm2 logs",
      "dev": "nodemon src/server.ts",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "ts:check": "tsc -b",
      "check": "pnpm lint && pnpm ts:check",
      "build": "tsc -b",
      "db:push": "drizzle-kit push:sqlite --config drizzle.config.ts",
      "db:generate": "drizzle-kit generate:sqlite --config drizzle.config.ts"
   },
   "dependencies": {
      "@libsql/client": "^0.6.0",
      "@trpc/client": "^10.29.1",
      "@trpc/server": "^10.29.1",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "drizzle-orm": "^0.30.9",
      "drizzle-zod": "^0.5.1",
      "express": "^4.18.2",
      "express-serve-static-core": "^0.1.1",
      "uploadthing": "^6.9.0",
      "zod": "^3.21.4"
   },
   "devDependencies": {
      "@react-vite-trpc/config": "workspace:*",
      "@react-vite-trpc/tsconfig": "workspace:*",
      "@swc/core": "^1.3.62",
      "@types/cookie-parser": "^1.4.3",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.17",
      "@types/fs-extra": "^11.0.1",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.16.17",
      "cross-env": "^7.0.3",
      "dotenv": "^16.1.4",
      "drizzle-kit": "^0.20.17",
      "envalid": "^7.3.1",
      "fs-extra": "^11.1.1",
      "module-alias": "^2.2.3",
      "nodemon": "^2.0.22",
      "pm2": "^5.3.0",
      "typescript": "^4.9.5"
   }
}
