{
   "tabWidth": 3,
   "printWidth": 120,
   "semi": true,
   "singleQuote": true,
   "arrowParens": "avoid",
   "trailingComma": "es5",
   "importOrderSeparation": true,
   "importOrderSortSpecifiers": true,
   "importOrder": [
      "<THIRD_PARTY_MODULES>",

      "^dotenv/config$",

      "^(.*)aliases$",

      "core",

      "^env$",

      "^@studio(.*)",

      "^trpc(.*)",
      "^trpc$",

      "^middlewares(.*)",

      "^helpers$",
      "(.*)helpers$",

      "^utils$",
      "(.*)utils$",

      "^types(.*)",
      "(.*)types$",

      "^[./]"
   ]
}
